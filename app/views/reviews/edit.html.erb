<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @review, url: review_path, method: :patch do |f|%>
      <%= f.label :title%>
      <%= f.text_field :title, class: 'fomr-control' %>
      <%= f.label :reviews  %>
      <%= f.text_field :content, class: 'form-control' %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
